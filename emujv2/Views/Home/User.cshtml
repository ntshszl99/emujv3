@using System.Data
@model DataSet


@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="app-content main-content">
    <div class="side-app">
        <div class="container-fluid main-container">

            <!--Page header-->
            <div class="page-header">
                <div class="page-leftheader">
                    <h4 class="page-title">User</h4>
                </div>
                <div class="page-rightheader ms-auto d-lg-flex d-none">
                    <ol class="breadcrumb">
                        <li></li>
                        <li class="breadcrumb-item"><a href="/Home/Report"><span class="breadcrumb-icon">Form</span></a></li>
                        <li class="breadcrumb-item"><a href="/Home/Report"><span class="breadcrumb-icon">Report</span></a></li>
                        <li class="breadcrumb-item active font-weight-bold" aria-current="page">Checklist</li>
                    </ol>
                </div>
            </div>
            <!--End Page header-->

            <div class="row">
                <div class="col-lg-12 col-md-12">
                    <div class="card">
                        <div class="card-header">
                            <h3 class="card-title">Registered User</h3>
                        </div>
                        <div class="card-body">
                            <div class="mb-3 m-0">
                                <div class="input-group rounded">
                                    <table cellpadding="8" cellspacing="8">
                                        <tr>
                                            <th>CustomerId</th>
                                            <th>Name</th>
                                            <th>Country</th>
                                        </tr>
                                        @foreach (DataRow row in Model.Tables[0].Rows)
                                        {
                                            <tr>
                                                <td>@row["staff_name"]</td>
                                                <td>@row["staff_id"]</td>
                                                <td>@row["dept"]</td>
                                            </tr>
                                        }
                                    </table>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


